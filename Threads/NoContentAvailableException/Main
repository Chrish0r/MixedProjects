package noContentAvailableException;

import javax.naming.NotContextException;

public class Main {

	public static void main(String[] args) {
	
		Content c = new Content(4);
		
		
		// System.out.println(c.getContent());
		
		try {
			System.out.println(c.getContent());
		} catch(NoContentAvailableException e) {
			e.printStackTrace();
		} 

	}

}
